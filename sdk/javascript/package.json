{
  "name": "@mercadopago-nextjs/sdk",
  "version": "1.0.0",
  "description": "SDK JavaScript/TypeScript para integração com MercadoPago NextJS API",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "module": "dist/index.esm.js",
  "exports": {
    ".": {
      "import": "./dist/index.esm.js",
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "files": [
    "dist",
    "README.md"
  ],
  "scripts": {
    "build": "rollup -c",
    "build:watch": "rollup -c -w",
    "test": "jest",
    "test:watch": "jest --watch",
    "lint": "eslint src --ext .ts",
    "lint:fix": "eslint src --ext .ts --fix",
    "type-check": "tsc --noEmit",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "mercadopago",
    "payment",
    "subscription",
    "nextjs",
    "sdk",
    "typescript",
    "javascript"
  ],
  "author": "Andre Dev",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/andredev/mercadopago-nextjs-by-andredev.git"
  },
  "bugs": {
    "url": "https://github.com/andredev/mercadopago-nextjs-by-andredev/issues"
  },
  "homepage": "https://github.com/andredev/mercadopago-nextjs-by-andredev#readme",
  "devDependencies": {
    "@rollup/plugin-node-resolve": "^15.2.3",
    "@rollup/plugin-typescript": "^11.1.5",
    "@types/jest": "^29.5.8",
    "@typescript-eslint/eslint-plugin": "^6.12.0",
    "@typescript-eslint/parser": "^6.12.0",
    "eslint": "^8.54.0",
    "jest": "^29.7.0",
    "rollup": "^4.5.0",
    "ts-jest": "^29.1.1",
    "typescript": "^5.3.2"
  },
  "peerDependencies": {
    "typescript": ">=4.0.0"
  },
  "peerDependenciesMeta": {
    "typescript": {
      "optional": true
    }
  }
}
